{
  "version": "1.0",
  "loggers": {
    "default": {
      "level": "info",
      "appenders": ["console", "file"]
    }
  },
  "appenders": {
    "console": {
      "type": "console"
    },
    "file": {
      "type": "file",
      "filename": "logs/app.log",
      "maxSize": 10485760,
      "backups": 5,
      "compress": true
    }
  }
}